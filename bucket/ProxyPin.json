  "pre_install": [
    "$bucket=(scoop bucket list | where-Object { $_.Source -match 'Scoop-Store'})[0].Name",
    ". \"$bucketsdir\\$bucket\\scripts\\utils.ps1\"",
    "persist_file -dir @(\"$env:APPDATA\\com.proxy.pin\") @(\"$persist_dir\\APPDATA\\com.proxy.pin\")",
